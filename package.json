  "bugs": {
    "url": "https://github.com/ahjuice/dashlane-json-to-csv/issues"
  },
  "homepage": "https://github.com/ahjuice/dashlane-json-to-csv#readme",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.10.1",
    "@types/sinon-chai": "^3.2.4",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "sinon-chai": "^3.5.0",
    "ts-node": "^9.0.0",
    "ts-sinon": "^2.0.1",
